{# templates/form/article_type.html.twig #}
{% block title %}Register{% endblock %}
{% block body%}
<h1>Formulaire d'insertion d'un article</h1>
{{ form_start(form) }}

{{ form_row(form.nom) }}
{{ form_row(form.prix) }}
{{ form_row(form.quantite) }}
{{ form_row(form.description) }}

<button type="submit">Enregistrer</button>

{{ form_end(form) }}
{% endblock%}